include _string

block vars
  - var lang = "ja"
  - var pageName = "game"

doctype html
html(lang=`${lang}`)
  head
    include components/_head

    link(rel="stylesheet" href=`/assets/css/game.scss`)
    script(src=`/assets/js/game.js`)

  body

    header
      include components/_toolbar

    main
      include components/_game-list

    footer
      include components/_footer
