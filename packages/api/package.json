{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "start": "run-p dev serve",
    "dev": "tsc -w",
    "serve": "firebase serve --only hosting:api-dev,functions",
    "shell": "npm run build && firebase functions:shell",
    "deploy-dev": "firebase deploy --only hosting:api,functions --project sokontokoro-factory-develop",
    "deploy-pro": "firebase deploy --only hosting:api,functions --project sokontokoro-factory",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0",
    "googleapis": "^47.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "firebase-functions-test": "^0.1.6",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  }
}
