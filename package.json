{
  "name": "www.sokontokoro-factory.net",
  "version": "1.10.0",
  "description": "sokontokoro factory web site",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run build:pug -- -w & webpack-dev-server -w",
    "clean": "rm -rf dist",
    "deploy":
      "    yarn build     && gsutil -m rsync -d -r dist/ gs://www-dev.sokontokoro-factory.net/",
    "deploy:pro":
      "yarn build:pro && gsutil -m rsync -d -r dist/ gs://www.sokontokoro-factory.net/",
    "build":
      "yarn clean && yarn build:pug && yarn build:js && npm run copy-assets && npm run imagemin",
    "build:pro": "NODE_ENV=production npm run build",
    "build:pug": "pug src/ --out dist --obj package.json",
    "build:js": "webpack",
    "copy-assets": "cpx \"src/assets/**/*\" dist/assets",
    "imagemin": "imagemin dist/assets/image/* --out-dir=dist/assets/image",
    "format": "prettier 'src/**/*.{js,scss}' --write",
    "precommit": "pretty-quick --staged"
  },
  "repository": {
    "type": "git",
    "url": "github"
  },
  "author": "Tatuya Yamamoto",
  "license": "MIT",
  "dependencies": {
    "material-components-web": "^0.34.1",
    "reset-css": "^3.0.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "css-loader": "^0.28.11",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "imagemin-cli": "^3.0.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "pretty-quick": "^1.6.0",
    "pug": "^2.0.0-rc.4",
    "pug-cli": "github:pugjs/pug-cli#master",
    "pug-loader": "^2.4.0",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  }
}
